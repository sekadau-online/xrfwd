# SSH Configuration
SSH_HOST=your-vps-ip
SSH_PORT=22
SSH_USERNAME=root
SSH_PRIVATE_KEY_PATH=/home/user/.ssh/id_rsa
SSH_PASSPHRASE=  # Kosongkan jika tidak ada

# Forwarding Configuration
LOCAL_HOST=localhost
LOCAL_PORT=3000
REMOTE_HOST=0.0.0.0
REMOTE_PORT=8080

# Application Configuration
HEALTH_CHECK_INTERVAL=30
RECONNECT_DELAY=5
LOG_LEVEL=info

# Web Server (optional)
WEB_INTERFACE=127.0.0.1
WEB_PORT=8081
